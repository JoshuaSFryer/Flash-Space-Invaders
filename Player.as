package  {	import flash.display.MovieClip;	import flash.events.Event;		public class Player extends MovieClip {				public var xspeed:Number = 0;		public var health:Number;				public function Player() {			this.addEventListener(Event.ENTER_FRAME, moveMe);			health = C.playerInitialHealth;		}					public function moveMe(e:Event):void {			//xspeed is controlled via keyboard in main timeline			this.x += this.xspeed;			//stop the player from going offscreen			if (this.x >= (800 - this.width/2)) {				this.x = 800 - this.width/2;			}			else if (this.x <= this.width/2) {				this.x = this.width/2;			}		}	}}